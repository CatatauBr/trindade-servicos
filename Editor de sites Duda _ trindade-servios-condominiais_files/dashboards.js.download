   &.white,\n        &.disabled {\n            color: inherit;\n        }\n    }\n    &-h1 {\n        font-size: $font-size-h1;\n        font-family: $duda-title-font;\n        font-weight: bold;\n        line-height: 56px;\n    }\n    &-h2 {\n        font-size: $font-size-h2;\n        font-family: $duda-title-font;\n        font-weight: bold;\n        line-height: 48px;\n    }\n    &-h3 {\n        font-size: $font-size-h3;\n        font-family: $duda-title-font;\n        font-weight: bold;\n        line-height: 40px;\n    }\n    &-h4 {\n        font-size: $font-size-h4;\n        font-family: $duda-title-font;\n        font-weight: 500;\n        line-height: 32px;\n    }\n    &-h5 {\n        font-size: $font-size-h5;\n        font-family: $duda-title-font;\n        font-weight: normal;\n        line-height: 28px;\n    }\n    &-italic {\n        font-size: $font-size-italic;\n        font-family: $duda-font;\n        font-weight: normal;\n        font-style: italic;\n        line-height: 20px;\n    }\n    &-footer {\n        @include typography-12r();\n    }\n    &-display-00 {\n        @include typography-cal60();\n    }\n    &-display-0 {\n        @include typography-cal40();\n    }\n    &-display-1 {\n        @include typography-cal32();\n    }\n    &-display-2 {\n        @include typography-24m();\n\n        &-bold {\n            @include typography-cal24();\n        }\n    }\n    &-display-3 {\n        @include typography-20m();\n\n        &-bold {\n            @include typography-cal20();\n        }\n\n        &_5 {\n            @include typography-cal12-cap();\n        }\n    }\n    &-display-4 {\n        @include typography-10b-cap();\n    }\n    &-description {\n        @include typography-20r();\n    }\n    &-he-0 {\n        @include typography-18m();\n    }\n    &-he-1 {\n        @include typography-16b();\n    }\n    &-he-2 {\n        @include typography-14m();\n    }\n    &-he-2-bold {\n        @include typography-14b();\n    }\n    &-he-3 {\n        @in