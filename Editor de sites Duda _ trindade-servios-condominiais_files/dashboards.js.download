width: 30px;\n    width: auto;\n    font-size: 12px;\n    display: flex;\n  }\n\n  &.selected {\n    background-color: $light-gray;\n    box-shadow: $default-inset-box-shadow;\n    color: $text-on-light;\n    fill: $text-on-light;\n  }\n}\n\n@mixin dynamic-background-color($color: $primary-color, $opacity: 0.05) {\n  position: relative;\n  z-index: 0;\n  &::before,\n  &::after {\n    content: '';\n    opacity: $opacity;\n    pointer-events: none;\n    position: absolute;\n    border-radius: inherit;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    z-index: -1;\n    background-color: $color;\n  }\n}\n\n.arrow {\n  border: 9px solid black;\n  transform-origin: 0 0;\n  position: absolute;\n  z-index: 101;\n  backface-visibility: hidden;\n  width: 0;\n}\n\n.duda-left-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  box-shadow: -1px 1px 1px 0px rgba(0, 0, 0, 0.1);\n  transform: rotate(59deg) skewY(-14deg) skewX(10deg);\n}\n\n.duda-right-arrow {\n  composes: arrow;\n  transform: rotate(-17deg) skewY(-16deg) skewX(-18deg);\n  box-shadow: -1px 3px 0px -1px rgba(0, 0, 0, 0.1);\n  border-color: #f2f2f2 #f2f2f2 transparent transparent;\n}\n\n.duda-top-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(136deg) skewY(0deg) skewX(2deg);\n  box-shadow: -1px 1px 0 0px #c5c3c3;\n}\n\n.duda-bottom-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(-60deg) skewY(20deg) skewX(-11deg);\n  box-shadow: -1px 3px 2px -1px rgba(0, 0, 0, 0.24);\n}\n\n@mixin placeholder {\n  &::-webkit-input-placeholder {\n    @content;\n  }\n  &:-moz-placeholder {\n    @content;\n  }\n  &::-moz-placeholder {\n    @content;\n  }\n  &:-ms-input-placeholder {\n    @content;\n  }\n}\n\n@mixin ellipsis {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n","@import 'deprecated-colors';\n@import 'deprecated-variables';\n@import 'primitive-colors';\n\n//Custom branding colors\n$primary-color: var(--button-color);\n$secondary-color: var(--text-on-light);\n$text-on-primary-color: var(--text-on-button);\n$top-bar-color: var(--header-color);\n\n// Platform colors\n$bg-white-color: $white;\n$bg-grey-color: $grey-100;\n$bg-med-grey-color: $grey-200;\n$bg-off-white-color: $off-white-50;\n$bg-med-off-white-color: $off-white-100;\n$bg-dark-grey-color: $grey-500;\n\n// Border colors\n$border-color: $grey-300;\n$border-light-color: $grey-200;\n$border-dark-color: $grey-600;\n$active-border-color: $grey-400;\n$divider-color: $grey-200;\n$border-white-color: $white;\n$border-off-white-color: $off-white-300;\n\n// deprecated colors\n$selectable-item-color: $grey-600;\n$error-color: $red-500;\n$error-bg-color: $red-50;\n$success-color: $green-400;\n$success-bg-color: $green-50;\n\n// Typography colors\n$primary-text-color: $grey-800;\n$secondary-text-color: $grey-600;\n$hint-color: $grey-500;\n$text-white-color: $white;\n\n// Icon color\n$icon-color: $grey-600;\n$icon-dark-color: $grey-800;\n$icon-grey-color: $grey-500;\n$icon-light-color: $white;\n\n// Indication\n$link-color: $blue-600;\n$disabled-item-color: $grey-300;\n$positive-light-color: $green-50;\n$positive-subtle-color: $green-100;\n$positive-color: $green-500;\n$positive-med-color: $green-700;\n$positive-dark-color: $green-800;\n$negative-light-color: $red-50;\n$negative-subtle-color: $red-100;\n$negative-color: $red-600;\n$negative-med-color: $red-700;\n$negative-dark-color: $red-800;\n$info-bg-color: $blue-50;\n$info-color: $blue-400;\n$info-med-color: $blue-700;\n$info-dark-color: $blue-800;\n$ai-bg-color: $purple-50;\n$ai-color: $purple-500;\n$ai-med-color: $purple-100;\n$ai-dark-color: $purple-700;\n$warning-color: $yellow-500;\n$warning-med-color: $yellow-300;\n$warning-bg-color: $yellow-100;\n$flex-color: $blue-500;\n$flex-dark-color: $blue-600;\n$pending-bg-color: $orange-100;\n$pending-color: $orange-600;\n\n// Decorative palette colors\n$dark-brown-color: var(--eclectic-1, $eclectic-1);\n$greige-color: var(--eclectic-2, $eclectic-2);\n$orange-color: var(--eclectic-3, $eclectic-3);\n$dark-pink-color: var(--eclectic-4, $eclectic-4);\n$light-pink-color: var(--eclectic-5, $eclectic-5);\n$purple-color: var(--eclectic-6, $eclectic-6);\n$bright-purple-color: var(--eclectic-7, $eclectic-7);\n$bright-blue-color: var(--eclectic-8, $eclectic-8);\n$baby-blue-color: var(--eclectic-9, $eclectic-9);\n$lemon-yellow-color: var(--eclectic-10, $eclectic-10);\n$olive-green-color: var(--eclectic-11, $eclectic-11);\n$bright-green-color: var(--eclectic-12, $eclectic-12);\n$light-orange-color: var(--eclectic-13, $eclectic-13);\n$baby-pink-color: var(--eclectic-14, $eclectic-14);\n$lilach-color: var(--eclectic-15, $eclectic-15);\n$light-purple-color: var(--eclectic-16, $eclectic-16);\n$light-green-color: var(--eclectic-17, $eclectic-17);\n$light-olive-color: var(--eclectic-18, $eclectic-18);\n$light-yellow-color: var(--eclectic-19, $eclectic-19);\n\n// Component colors - should not be used anywhere else apart from table\n$header-color: $grey-100;\n$table-cell-hover-color: $grey-100;\n$exit-button-color: $grey-600;\n\n//Tag labels\n$tag-lilac-color: $purple-300;\n$tag-red-color: $pink-400;\n$tag-orange-color: $orange-400;\n$tag-light-green-color: $green-400;\n$tag-dark-blue-color: $blue-200;\n$tag-dark-green-color: $green-600;\n$tag-grey-color: $grey-600;\n$tag-purple-color: $purple-700;\n$tag-light-grey-color: $grey-100;\n\n// Status colors\n$status-yellow-bg-color: $yellow-100;\n$status-yellow-color: $orange-700;\n$status-yellow-border-color: $yellow-300;\n$status-dark-grey-bg-color: $grey-300;\n$status-dark-grey-color: $grey-700;\n$status-dark-grey-border-color: $grey-400;\n$status-neutral-bg-color: $grey-100;\n$status-neutral-color: $grey-600;\n$status-neutral-border-color: $grey-200;\n$status-positive-bg-color: $green-50;\n$status-positive-color: $green-700;\n$status-positive-border-color: $green-100;\n$status-error-bg-color: $red-50;\n$status-error-color: $red-700;\n$status-error-border-color: $red-100;\n$status-purple-bg-color: $purple-50;\n$status-purple-color: $purple-700;\n$status-purple-border-color: $purple-100;\n$status-pending-bg-color: $orange-50;\n$status-pending-color: $orange-700;\n$status-pending-border-color: $orange-100;\n$status-turquoise-bg-color: $blue-50;\n$status-turquoise-color: $blue-800;\n$status-turquoise-border-color: $blue-100;\n\n// GENERIC SHADOWS\n$shadow-base-color: #18274b;\n$shadow-1: 0px 2.25px 2.25px -1.125px rgba($shadow-base-color, 0.04),\n    0px 1.125px 2.25px -1.125px rgba($shadow-base-color, 0.08);\n\n$shadow-2: 0px 4.5px 9px -3.375px rgba($shadow-base-color, 0.04),\n    0px 3.375px 4.5px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-3: 0px 5.625px 18px -2.25px rgba($shadow-base-color, 0.06),\n    0px 3.375px 7.875px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-4: 0px 7.875px 36px -2.25px rgba($shadow-base-color, 0.08),\n    0px 4.5px 12.375px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-5: 0px 10.125px 49.5px -2.25px rgba($shadow-base-color, 0.12),\n    0px 4.5px 15.75px -3.375px rgba($shadow-base-color, 0.1);\n\n$top-bar-items-depths: base, buttons, banners;\n\n// SPACING\n$base-spacing: 8px;\n$spacing-0: 0;\n$spacing-half: 4px;\n$spacing-1: $spacing-half * 2;\n$spacing-1-half: $spacing-1 + $spacing-half;\n$spacing-2: $spacing-1 * 2;\n$spacing-3: $spacing-1 * 3;\n$spacing-4: $spacing-1 * 4;\n$spacing-5: $spacing-1 * 5;\n$spacing-6: $spacing-1 * 6;\n$spacing-7: $spacing-1 * 7;\n$spacing-8: $spacing-1 * 8;\n$spacing-2-half: 18px;\n\n// BORDERS\n/**\n* @deprecated please use $border-radius-s instead\n */\n$default-border-radius: 4px;\n$border-radius-xs: 2px;\n$border-radius-s: 4px;\n$border-radius-sm: 6px;\n$border-radius-m: 8px;\n$border-radius-l: 16px;\n$border-radius-full: 1000px;\n/**\n* @deprecated please use $border-radius-s or $border-radius-m instead, we don't have 6px radius anymore\n */\n$floating-elements-radius: 6px;\n$default-border: 1px solid $border-color;\n$active-border: 1px solid $active-border-color;\n\n// FONTS\n$duda-title-font: var(--d-title-font, 'Roboto');\n$duda-font: var(--d-font, 'Source Sans Pro', sans-serif);\n\n// typography font sizes\n$font-size-h1: var(--font-size-h1, 48px);\n$font-size-h2: var(--font-size-h2, 40px);\n$font-size-h3: var(--font-size-h3, 32px);\n$font-size-h4: var(--font-size-h4, 24px);\n$font-size-h5: var(--font-size-h5, 20px);\n$font-size-h6: var(--font-size-h6, 16px);\n$font-size-pageDescription: var(--font-size-pageDescription, 16px);\n$font-size-bodySubtitle: var(--font-size-bodySubtitle, 14px);\n$font-size-body: var(--font-size-body, 14px);\n$font-size-italic: var(--font-size-italic, 14px);\n$font-size-tiny: var(--font-size-tiny, 13px);\n$font-size-footer: var(--font-size-footer, 12px);\n\n// new font-sizes:\n$font-size-display-1: var(--font-size-display-1, 32px);\n$font-size-display-2: var(--font-size-display-1, 24px);\n$font-size-display-3: var(--font-size-display-1, 20px);\n$font-size-display-4: var(--font-size-display-1, 10px);\n$font-size-he-1: var(--font-size-he-1, 16px);\n$font-size-he-2: var(--font-size-he-2, 14px);\n$font-size-he-3: var(--font-size-he-3, 13px);\n$font-size-he-4: var(--font-size-he-4, 12px);\n$font-size-plain-text: var(--font-size-plain-text, 16px);\n$font-size-body-text: var(--font-size-body-text, 13px);\n$font-size-tiny-text: var(--font-size-tiny-text, 12px);\n$font-size-mini: var(--font-size-mini, 10px);\n\n$widget-width: 344px;\n$widget-width-mid-size: 449px;\n$widget-width-large-size: 616px;\n$widget-double-width: 2 * $widget-width;\n$widget-width-xl-size: 963px;\n$widget-height: 536px;\n\n$default-widget-padding: 31px;\n\n$container-width-regular-size: $widget-width - $default-widget-padding;\n$container-width-large-size: $widget-width-large-size - $default-widget-padding;\n$container-width-double-size: $widget-double-width - $default-widget-padding;\n\n$left-panel-inner-width: 302px;\n\n$settings-panel-large: 617px;\n\n$editor-left-panel-width: var(--editor-left-panel-width, 98px);\n\n$top-bar-height: 50px;\n$top-bar-message-height: 50px;\n\n$input-height: 35px;\n\n// for tablet portrait\n$tablet-editing-breakpoint: 1000px;\n$zoom-coefficient: 1.5;\n$landscape-media-query: 'screen and (min-width: $tablet-editing-breakpoint), screen and (orientation: landscape)';\n$left-panel-width: 800px;\n$left-panel-list-width: 270px;\n$left-panel-right-width: $left-panel-width - $left-panel-list-width - 50px;\n\n// duda-go\n$default-padding: 24px;\n$default-widget-padding-top: 16px;\n$default-mobile-shadow: 0 -2px 12px 0 rgba(47, 55, 58, 0.5);\n$default-mobile-shadow-top: 0 2px 12px 0 rgba(47, 55, 58, 0.5);\n$input-border-bottom: 1px solid $dark-gray;\n$mobile-popups-box-shadow: 1px 1px 21px rgba(0, 0, 0, 0.3);\n$default-panels-border-radius: 9px;\n\n//right-design-panel\n$right-design-panel-rows-margin: 20px;\n\n// overview\n$overview-pages-panel-width: 266px;\n\n// MIXINS\n@mixin rotate($deg: 90deg) {\n    -webkit-transform: rotate($deg);\n    -moz-transform: rotate($deg);\n    -ms-transform: rotate($deg);\n    -o-transform: rotate($deg);\n    transform: rotate($deg);\n}\n\n@mixin transition($length: 1s, $delay: 0s, $easing: ease, $properties: all) {\n    transition: $length $easing $delay $properties;\n}\n","// Colors here are deprecated! Please use new variables in the variables.scss\n// If you need to use some of these variables please contact designer or repo maintainer\n\n// Common css variables\n$button-color: var(--button-color); // Primary\n$button-hover-color: var(--button-hover-color);\n$text-on-light: var(--text-on-light); // Secondary // the orange colors for SMBs\n$text-on-button: var(--text-on-button); // Text on primary\n$text-on-dark: var(--text-on-dark);\n\n//COLORS THEME\n$duda-orange: #f66035;\n$duda-orange-hover: #f86f4c;\n$link: #1870cb; // use $link-color instead\n$duda-background-color: #f5f7f7;\n$flex-blue: #198df8;\n\n//grays - sorted by brightness\n$font-light: #ffffff;\n$light-gray: #f5f7f7;\n$disabled-bg: #e9eeef;\n$light-border: #e8e8e8;\n$dark-border: #dde4e7;\n$mid-dark-gray: #d2d2d2;\n$duda-gray: #ced6d9;\n$light-link-gray: #828894;\n$darker-gray: #bfbfbf;\n$text-dark-gray: #a1abb0;\n$text-dark: #383838;\n$font-light-gray: #828894; // use $secondary-text-color instead\n$secondary-grey: #828894;\n$clickable-item: #616c79;\n$clickable-item-hover: #828894;\n$grayer-gray: #526065;\n$background-topbar-hover: #394347;\n$bg-gray: #374043;\n$dark-gray: #313131;\n$theme-dark: #2f373a;\n$background-top-toolbar: #2b3235;\n$active-gray: #292f32;\n$separator-dark: #000000;\n$font-black: #000000;\n$pale-grey: #fafafb;\n$header-table: #617379;\n$header-background: #f5f7f7;\n\n$dark-toolbar: #2a2a2a;\n$dark-toolbar-item-active: #55565a;\n$dark-toolbar-text: #c4c4c4;\n$dark-toolbar-text-active: #ffffff;\n$dark-toolbar-bright-item-active: #ebecf0;\n$dark-toolbar-bright-text: #2f373a;\n\n$font-dark-gray: $dark-gray;\n\n$red-error: #e7463a;\n$deep-red: #d0021b;\n$pink-background: #fff0e4;\n$pink-stroke: #f0e1d5;\n$duda-green: #3ac280;\n$duda-green-light: #3fcf89;\n$duda-yellow: #e8c72e;\n$duda-blue: #0091ff;\n$duda-destructive: #d30000;\n$duda-destructive-light: #de2727;\n","// Variables here are deprecated! Please use new variables in the variables.scss\n// If you need to use some of these variables please contact designer or repo maintainer\n\n$shadow-color: rgba(64, 65, 71, 0.3);\n\n//BOX SHADOWS THEME\n$default-box-shadow: 1px 1px 2px 0 rgba(75, 75, 85, 0.2);\n$default-disabled-box-shadow: 1px 1px 2px 0 rgba(75, 75, 85, 0.4);\n$default-inset-box-shadow: inset 1px 1px 3px rgba(0, 0, 0, 0.15);\n$tooltips-box-shadow: 1px 1px 1px 0px rgba(64, 65, 71, 0.3);\n$top-window-box-shadow: -2px 3px 12px -1px rgba(100, 100, 100, 0.3);\n$popups-box-shadow: 1px 1px 6px rgba(0, 0, 0, 0.3);\n$right-panels-box-shadow: 1px 1px 6px rgba(0, 0, 0, 0.3);\n$left-panels-box-shadow: -1px 1px 6px rgba(0, 0, 0, 0.3);\n$contentmenu-box-shadow: 4px 4px 8px 0px rgba(64, 65, 71, 0.28);\n\n// RTL BOX SHADOWS\n$default-box-shadow-rtl: -1px 1px 2px 0 rgba(75, 75, 85, 0.2);\n$default-disabled-box-shadow-rtl: -1px 1px 2px 0 rgba(75, 75, 85, 0.4);\n$default-inset-box-shadow-rtl: inset -1px 1px 3px rgba(0, 0, 0, 0.15);\n$tooltips-box-shadow-rtl: -1px 1px 1px 0px $shadow-color;\n$top-window-box-shadow-rtl: 2px 3px 12px -1px rgba(100, 100, 100, 0.3);\n$popups-box-shadow-rtl: -1px 1px 6px rgba(0, 0, 0, 0.3);\n$contentmenu-box-shadow-rtl: -4px 4px 8px 0px rgba(64, 65, 71, 0.28);\n","@import '~@duda-co/styles/variables';\n@import '~@duda-co/styles/common';\n\n$overlay-color: rgba(12, 16, 28, 0.6);\n$modal-background-color: $bg-white-color;\n\n.small {\n    width: 480px;\n}\n\n.medium {\n    width: 740px;\n}\n\n.large {\n    width: 1000px;\n}\n\n.xLarge {\n    width: 1356px;\n}\n\n.overlay {\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    z-index: 100;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    background-color: $overlay-color;\n    pointer-events: none;\n    opacity: 0;\n    transition: opacity 0.2s ease-in-out;\n\n    &.shown {\n        pointer-events: auto;\n        opacity: 1;\n    }\n\n    .main {\n        display: flex;\n        flex-direction: column;\n        max-height: 90vh;\n        background-color: $modal-background-color;\n        position: relative;\n        overflow: hidden;\n        border-radius: 12px;\n        opacity: 1;\n        transform: scale(1);\n        transition: transform 0.2s ease-in-out, opacity 0.2s ease-in-out;\n        box-shadow: $shadow-5;\n\n        &[aria-hidden] {\n            opacity: 0;\n            transform: scale(0.6);\n        }\n\n        .header {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            padding-inline-start: $spacing-3;\n            padding-inline-end: $spacing-3;\n            padding-block-start: $base-spacing * 2;\n            padding-block-end: $base-spacing * 2;\n            border-bottom: 1px solid $border-light-color;\n\n            .title {\n                text-align: start;\n            }\n\n            .close-button {\n                margin-inline-start: auto;\n                color: $icon-dark-color;\n                cursor: pointer;\n            }\n\n            .header-actions {\n                margin-inline-start: auto;\n                display: flex;\n                align-items: center;\n            }\n\n            .help-button {\n                margin-inline-end: $spacing-1;\n                color: $icon-dark-color;\n                cursor: pointer;\n            }\n        }\n\n        .body,\n        .body-with-scroll {\n            display: flex;\n            flex-direction: column;\n            box-sizing: border-box;\n            padding-block-start: $base-spacing * 3;\n            padding-block-end: $base-spacing * 3;\n            padding-inline-start: $spacing-3;\n            padding-inline-end: $spacing-3;\n            overflow-y: auto;\n            @include scrollbar-mixin();\n        }\n\n        .body-with-scroll {\n            overflow-y: auto;\n\n            &.bottomBorder {\n                border-bottom: 1px solid $divider-color;\n            }\n        }\n\n        .actions {\n            margin-block-start: $base-spacing * 2;\n            margin-block-end: $base-spacing * 2;\n            padding-inline-start: $spacing-3;\n            padding-inline-end: $spacing-3;\n\n            .confirmation-actions {\n                display: flex;\n                justify-content: flex-end;\n\n                .button {\n                    margin-inline-start: 8px;\n                    min-width: 85px;\n                }\n            }\n        }\n\n        .footer {\n            font-size: 12px;\n            text-align: center;\n            padding-block-start: $base-spacing * 2;\n            padding-block-end: $base-s