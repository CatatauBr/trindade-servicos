sition: absolute;\n            right: 24px;\n            transition: 0.2s transform;\n        }\n\n        .promotionalTag {\n            position: absolute;\n            right: 48px;\n        }\n\n        [data-auto='icon'],\n        .externalIcon,\n        .arrow {\n            color: $clickable-item;\n        }\n\n        &:hover {\n            color: $selected-color;\n\n            [data-auto='icon'] {\n                color: $selected-color;\n            }\n        }\n    }\n\n    .accordionContent {\n        overflow: hidden;\n        visibility: hidden;\n        display: none;\n\n        .pageNavItem {\n            position: relative;\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            height: 42px;\n            color: $clickable-item;\n            font-size: 13px;\n            cursor: pointer;\n            transition: all 0.4s ease;\n            padding-inline-start: $panel-padding-start;\n            padding-inline-end: $panel-padding-end;\n            font-family: $duda-title-font;\n            font-weight: 500;\n\n            &:first-child {\n                margin-block-start: 8px;\n            }\n\n            .title {\n                margin-inline-start: calc(61px - #{$panel-padding-start});\n            }\n\n            &:hover {\n                background: $pale-grey;\n            }\n\n            &.isSelected {\n                background: #f5f5f7;\n                @extend %marker;\n            }\n        }\n    }\n\n    &.isSelected {\n        .accordionItem {\n            [data-auto='icon']:not(.arrow) {\n                color: $selected-color;\n            }\n\n            &.isRootComponent {\n                @extend %marker;\n\n                &:before {\n                    top: 2px;\n                }\n            }\n        }\n\n        &.isAccordionOpen {\n            .accordionItem .arrow {\n                transform: rotate(90deg);\n            }\n\n            .accordio