a(47, 55, 58, 0.5);\n$input-border-bottom: 1px solid $dark-gray;\n$mobile-popups-box-shadow: 1px 1px 21px rgba(0, 0, 0, 0.3);\n$default-panels-border-radius: 9px;\n\n//right-design-panel\n$right-design-panel-rows-margin: 20px;\n\n// overview\n$overview-pages-panel-width: 266px;\n\n// MIXINS\n@mixin rotate($deg: 90deg) {\n    -webkit-transform: rotate($deg);\n    -moz-transform: rotate($deg);\n    -ms-transform: rotate($deg);\n    -o-transform: rotate($deg);\n    transform: rotate($deg);\n}\n\n@mixin transition($length: 1s, $delay: 0s, $easing: ease, $properties: all) {\n    transition: $length $easing $delay $properties;\n}\n","// Colors here are deprecated! Please use new variables in the variables.scss\n// If you need to use some of these variables please contact designer or repo maintainer\n\n// Common css variables\n$button-color: var(--button-color); // Primary\n$button-hover-color: var(--button-hover-color);\n$text-on-light: var(--text-on-light); // Secondary // the orange colors for SMBs\n$text-on-button: var(--text-on-button); // Text on primary\n$text-on-dark: var(--text-on-dark);\n\n//COLORS THEME\n$duda-orange: #f66035;\n$duda-orange-hover: #f86f4c;\n$link: #1870cb; // use $link-color instead\n$duda-background-color: #f5f7f7;\n$flex-blue: #198df8;\n\n//grays - sorted by brightness\n$font-light: #ffffff;\n$light-gray: #f5f7f7;\n$disabled-bg: #e9eeef;\n$light-border: #e8e8e8;\n$dark-border: #dde4e7;\n$mid-dark-gray: #d2d2d2;\n$duda-gray: #ced6d9;\n$light-link-gray: #828894;\n$darker-gray: #bfbfbf;\n$text-dark-gray: #a1abb0;\n$text-dark: #383838;\n$font-light-gray: #828894; // use $secondary-text-color instead\n$secondary-grey: #828894;\n$clickable-item: #616c79;\n$clickable-item-hover: #828894;\n$grayer-gray: #526065;\n$background-topbar-hover: #394347;\n$bg-gray: #374043;\n$dark-gray: #313131;\n$theme-dark: #2f373a;\n$background-top-toolbar: #2b3235;\n$active-gray: #292f32;\n$separator-dark: #000000;\n$font-black: #000000;\n$pale-grey: #fafafb;\n$header-ta