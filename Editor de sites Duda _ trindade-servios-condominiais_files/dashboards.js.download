n@import 'deprecated-variables';\n@import 'basic-typography';\n@import 'gradients';\n\n$scroll-bar-width: 8px;\n\n@mixin scrollbar-mixin() {\n  &::-webkit-scrollbar {\n    width: $scroll-bar-width;\n    height: $scroll-bar-width;\n    display: block;\n    background: none;\n  }\n\n  &::-webkit-scrollbar-button {\n    display: none;\n  }\n\n  &::-webkit-scrollbar-track {\n    background: none;\n  }\n\n  &::-webkit-scrollbar-track-piece {\n  }\n\n  &::-webkit-scrollbar-thumb {\n    border-radius: $border-radius-full;\n    border: 2px solid rgba(0, 0, 0, 0);\n    background-clip: padding-box;\n    background-color: transparent;\n    transition: all 0.1s ease-in-out;\n  }\n  &:hover::-webkit-scrollbar-thumb {\n    background-color: $darker-gray;\n  }\n\n  &::-webkit-scrollbar-corner {\n  }\n\n  &::-webkit-resizer {\n    padding-block-start: 20px;\n    padding-inline-end: 20px;\n    padding-block-end: 20px;\n    padding-inline-start: 20px;\n  }\n}\n\n.duda-scroll-bar {\n  @include scrollbar-mixin();\n}\n\n.small-button {\n  padding-block-start: 4px;\n  padding-inline-end: 4px;\n  padding-block-end: 4px;\n  padding-inline-start: 4px;\n  border-radius: $default-border-radius;\n  width: 30px;\n  height: 30px;\n  text-align: center;\n  font-size: 14px;\n  outline: none;\n  box-shadow: $default-box-shadow;\n  border: $default-border;\n  color: $clickable-item;\n  cursor: pointer;\n  box-sizing: border-box;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n@mixin small-button-states() {\n  &.disabled {\n    opacity: 0.5;\n    cursor: default;\n    box-shadow: $default-disabled-box-shadow;\n  }\n\n  &.text {\n    min-width: 30px;\n    width: auto;\n    font-size: 12px;\n    display: flex;\n  }\n\n  &.selected {\n    background-color: $light-gray;\n    box-shadow: $default-inset-box-shadow;\n    color: $text-on-light;\n    fill: $text-on-light;\n  }\n}\n\n@mixin dynamic-background-color($color: $primary-color, $opacity: 0.05) {\n  position: relative;\n 