px;\n  text-align: center;\n  font-size: 14px;\n  outline: none;\n  box-shadow: $default-box-shadow;\n  border: $default-border;\n  color: $clickable-item;\n  cursor: pointer;\n  box-sizing: border-box;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n@mixin small-button-states() {\n  &.disabled {\n    opacity: 0.5;\n    cursor: default;\n    box-shadow: $default-disabled-box-shadow;\n  }\n\n  &.text {\n    min-width: 30px;\n    width: auto;\n    font-size: 12px;\n    display: flex;\n  }\n\n  &.selected {\n    background-color: $light-gray;\n    box-shadow: $default-inset-box-shadow;\n    color: $text-on-light;\n    fill: $text-on-light;\n  }\n}\n\n@mixin dynamic-background-color($color: $primary-color, $opacity: 0.05) {\n  position: relative;\n  z-index: 0;\n  &::before,\n  &::after {\n    content: '';\n    opacity: $opacity;\n    pointer-events: none;\n    position: absolute;\n    border-radius: inherit;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    z-index: -1;\n    background-color: $color;\n  }\n}\n\n.arrow {\n  border: 9px solid black;\n  transform-origin: 0 0;\n  position: absolute;\n  z-index: 101;\n  backface-visibility: hidden;\n  width: 0;\n}\n\n.duda-left-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  box-shadow: -1px 1px 1px 0px rgba(0, 0, 0, 0.1);\n  transform: rotate(59deg) skewY(-14deg) skewX(10deg);\n}\n\n.duda-right-arrow {\n  composes: arrow;\n  transform: rotate(-17deg) skewY(-16deg) skewX(-18deg);\n  box-shadow: -1px 3px 0px -1px rgba(0, 0, 0, 0.1);\n  border-color: #f2f2f2 #f2f2f2 transparent transparent;\n}\n\n.duda-top-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(136deg) skewY(0deg) skewX(2deg);\n  box-shadow: -1px 1px 0 0px #c5c3c3;\n}\n\n.duda-bottom-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(-60deg) skewY(20deg) skewX(-11deg);\n  box-shadow: -1px 3px 2px -1px rgba(0, 0, 0, 0.24);\n}\n\n@mixin placeholder {\n  &::-webkit-input-placeholder {\n    @content;\n  }\n  &:-moz-placeholder {\n    @content;\n  }\n  &::-moz-placeholder {\n    @content;\n  }\n  &:-ms-input-placeholder {\n    @content;\n  }\n}\n\n@mixin ellipsis {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n","@import 'deprecated-colors';\n@import 'deprecated-variables';\n@import 'primitive-colors';\n\n//Custom branding colors\n$primary-color: var(--button-color);\n$secondary-color: var(--text-on-light);\n$text-on-primary-color: var(--text-on-button);\n$top-bar-color: var(--header-color);\n\n// Platform colors\n$bg-white-color: $white;\n$bg-grey-color: $grey-100;\n$bg-med-grey-color: $grey-200;\n$bg-off-white-color: $off-white-50;\n$bg-med-off-white-color: $off-white-100;\n$bg-dark-grey-color: $grey-500;\n\n// Border colors\n$border-color: $grey-300;\n$border-light-color: $grey-200;\n$border-dark-color: $grey-600;\n$active-border-color: $grey-400;\n$divider-color: $grey-200;\n$border-white-color: $white;\n$border-off-white-color: $off-white-300;\n\n// deprecated colors\n$selectable-item-color: $grey-600;\n$error-color: $red-500;\n$error-bg-color: $red-50;\n$success-color: $green-400;\n$success-bg-color: $green-50;\n\n// Typography colors\n$primary-text-color: $grey-800;\n$secondary-text-color: $grey-600;\n$hint-color: $grey-500;\n$text-white-color: $white;\n\n// Icon color\n$icon-color: $grey-600;\n$icon-dark-color: $grey-800;\n$icon-grey-color: $grey-500;\n$icon-light-color: $white;\n\n// Indication\n$link-color: $blue-600;\n$disabled-item-color: $grey-300;\n$positive-light-color: $green-50;\n$positive-subtle-color: $green-100;\n$positive-color: $green-500;\n$positive-med-color: $green-700;\n$positive-dark-color: $green-800;\n$negative-light-color: $red-50;\n$negative-subtle-color: $red-100;\n$negative-color: $red-600;\n$negative-med-color: $red-700;\n$negative-dark-color: $red-800;\n$info-bg-color: $blue