e-color: $blue-800;\n$status-turquoise-border-color: $blue-100;\n\n// GENERIC SHADOWS\n$shadow-base-color: #18274b;\n$shadow-1: 0px 2.25px 2.25px -1.125px rgba($shadow-base-color, 0.04),\n    0px 1.125px 2.25px -1.125px rgba($shadow-base-color, 0.08);\n\n$shadow-2: 0px 4.5px 9px -3.375px rgba($shadow-base-color, 0.04),\n    0px 3.375px 4.5px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-3: 0px 5.625px 18px -2.25px rgba($shadow-base-color, 0.06),\n    0px 3.375px 7.875px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-4: 0px 7.875px 36px -2.25px rgba($shadow-base-color, 0.08),\n    0px 4.5px 12.375px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-5: 0px 10.125px 49.5px -2.25px rgba($shadow-base-color, 0.12),\n    0px 4.5px 15.75px -3.375px rgba($shadow-base-color, 0.1);\n\n$top-bar-items-depths: base, buttons, banners;\n\n// SPACING\n$base-spacing: 8px;\n$spacing-0: 0;\n$spacing-half: 4px;\n$spacing-1: $spacing-half * 2;\n$spacing-2: $spacing-1 * 2;\n$spacing-3: $spacing-1 * 3;\n$spacing-4: $spacing-1 * 4;\n$spacing-5: $spacing-1 * 5;\n$spacing-6: $spacing-1 * 6;\n$spacing-7: $spacing-1 * 7;\n$spacing-8: $spacing-1 * 8;\n\n// BORDERS\n$default-border-radius: 4px;\n$floating-elements-radius: 6px;\n$default-border: 1px solid $border-color;\n$active-border: 1px solid $active-border-color;\n\n// FONTS\n$duda-title-font: var(--d-title-font, 'Roboto');\n$duda-font: var(--d-font, 'Source Sans Pro', sans-serif);\n\n// typography font sizes\n$font-size-h1: var(--font-size-h1, 48px);\n$font-size-h2: var(--font-size-h2, 40px);\n$font-size-h3: var(--font-size-h3, 32px);\n$font-size-h4: var(--font-size-h4, 24px);\n$font-size-h5: var(--font-size-h5, 20px);\n$font-size-h6: var(--font-size-h6, 16px);\n$font-size-pageDescription: var(--font-size-pageDescription, 16px);\n$font-size-bodySubtitle: var(--font-size-bodySubtitle, 14px);\n$font-size-body: var(--font-size-body, 14px);\n$font-size-italic: var(--font-size-italic, 14px);\n$font-size-tiny: var(--font-size-tiny, 13