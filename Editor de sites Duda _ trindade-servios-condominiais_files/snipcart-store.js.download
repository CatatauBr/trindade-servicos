t-scrollbar-thumb {\n    border-radius: 20px;\n    border: 3px solid rgba(0, 0, 0, 0);\n    background-clip: padding-box;\n    background-color: transparent;\n    transition: all 0.1s ease-in-out;\n  }\n  &:hover::-webkit-scrollbar-thumb {\n    background-color: $darker-gray;\n  }\n\n  &::-webkit-scrollbar-corner {\n  }\n\n  &::-webkit-resizer {\n    padding-block-start: 20px;\n    padding-inline-end: 20px;\n    padding-block-end: 20px;\n    padding-inline-start: 20px;\n  }\n}\n\n.duda-scroll-bar {\n  @include scrollbar-mixin();\n}\n\n.small-button {\n  padding-block-start: 4px;\n  padding-inline-end: 4px;\n  padding-block-end: 4px;\n  padding-inline-start: 4px;\n  border-radius: $default-border-radius;\n  width: 30px;\n  height: 30px;\n  text-align: center;\n  font-size: 14px;\n  outline: none;\n  box-shadow: $default-box-shadow;\n  border: $default-border;\n  color: $clickable-item;\n  cursor: pointer;\n  box-sizing: border-box;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n@mixin small-button-states() {\n  &.disabled {\n    opacity: 0.5;\n    cursor: default;\n    box-shadow: $default-disabled-box-shadow;\n  }\n\n  &.text {\n    min-width: 30px;\n    width: auto;\n    font-size: 12px;\n    display: flex;\n  }\n\n  &.selected {\n    background-color: $light-gray;\n    box-shadow: $default-inset-box-shadow;\n    color: $text-on-light;\n    fill: $text-on-light;\n  }\n}\n\n@mixin dynamic-background-color($color: $primary-color, $opacity: 0.05) {\n  position: relative;\n  z-index: 0;\n  &::before,\n  &::after {\n    content: '';\n    opacity: $opacity;\n    pointer-events: none;\n    position: absolute;\n    border-radius: inherit;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    z-index: -1;\n    background-color: $color;\n  }\n}\n\n.arrow {\n  border: 9px solid black;\n  transform-origin: 0 0;\n  position: absolute;\n  z-index: 101;\n  backface-visibility: hidden;\n  width: 0;\n}\n\n.duda-left-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  box-shadow: -1px 1px 1px 0px rgba(0, 0, 0, 0.1);\n  transform: rotate(59deg) skewY(-14deg) skewX(10deg);\n}\n\n.duda-right-arrow {\n  composes: arrow;\n  transform: rotate(-17deg) skewY(-16deg) skewX(-18deg);\n  box-shadow: -1px 3px 0px -1px rgba(0, 0, 0, 0.1);\n  border-color: #f2f2f2 #f2f2f2 transparent transparent;\n}\n\n.duda-top-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(136deg) skewY(0deg) skewX(2deg);\n  box-shadow: -1px 1px 0 0px #c5c3c3;\n}\n\n.duda-bottom-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(-60deg) skewY(20deg) skewX(-11deg);\n  box-shadow: -1px 3px 2px -1px rgba(0, 0, 0, 0.24);\n}\n\n@mixin placeholder {\n  &::-webkit-input-placeholder {\n    @content;\n  }\n  &:-moz-placeholder {\n    @content;\n  }\n  &::-moz-placeholder {\n    @content;\n  }\n  &:-ms-input-placeholder {\n    @content;\n  }\n}\n\n@mixin ellipsis {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n","// Colors here are deprecated! Please use new variables in the variables.scss\n// If you need to use some of these variables please contact designer or repo maintainer\n\n// Common css variables\n$button-color: var(--button-color); // Primary\n$button-hover-color: var(--button-hover-color);\n$text-on-light: var(--text-on-light); // Secondary // the orange colors for SMBs\n$text-on-button: var(--text-on-button); // Text on primary\n$text-on-dark: var(--text-on-dark);\n\n//COLORS THEME\n$duda-orange: #f66035;\n$duda-orange-hover: #f86f4c;\n$link: #1870cb; // use $link-color instead\n$duda-background-color: #f5f7f7;\n$flex-blue: #198df8;\n\n//grays - sorted by brightness\n$font-light: #ffffff;\n$light-gray: #f5f7f7;\n$disabled-bg: #e9eeef;\n$light-border: #e8e8e8;\n$dark-border: #dde4e7;\n$mid-dark-gray: #d2d2d2;\n$duda-gray: #ced6d9;\n$light-link-gray: #828894;\n$darker-gray