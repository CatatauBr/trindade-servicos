flex;\n  }\n\n  &.selected {\n    background-color: $light-gray;\n    box-shadow: $default-inset-box-shadow;\n    color: $text-on-light;\n    fill: $text-on-light;\n  }\n}\n\n@mixin dynamic-background-color($color: $primary-color, $opacity: 0.05) {\n  position: relative;\n  z-index: 0;\n  &::before,\n  &::after {\n    content: '';\n    opacity: $opacity;\n    pointer-events: none;\n    position: absolute;\n    border-radius: inherit;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    z-index: -1;\n    background-color: $color;\n  }\n}\n\n.arrow {\n  border: 9px solid black;\n  transform-origin: 0 0;\n  position: absolute;\n  z-index: 101;\n  backface-visibility: hidden;\n  width: 0;\n}\n\n.duda-left-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  box-shadow: -1px 1px 1px 0px rgba(0, 0, 0, 0.1);\n  transform: rotate(59deg) skewY(-14deg) skewX(10deg);\n}\n\n.duda-right-arrow {\n  composes: arrow;\n  transform: rotate(-17deg) skewY(-16deg) skewX(-18deg);\n  box-shadow: -1px 3px 0px -1px rgba(0, 0, 0, 0.1);\n  border-color: #f2f2f2 #f2f2f2 transparent transparent;\n}\n\n.duda-top-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(136deg) skewY(0deg) skewX(2deg);\n  box-shadow: -1px 1px 0 0px #c5c3c3;\n}\n\n.duda-bottom-arrow {\n  composes: arrow;\n  border-color: transparent transparent #f2f2f2 #f2f2f2;\n  transform: rotate(-60deg) skewY(20deg) skewX(-11deg);\n  box-shadow: -1px 3px 2px -1px rgba(0, 0, 0, 0.24);\n}\n\n@mixin placeholder {\n  &::-webkit-input-placeholder {\n    @content;\n  }\n  &:-moz-placeholder {\n    @content;\n  }\n  &::-moz-placeholder {\n    @content;\n  }\n  &:-ms-input-placeholder {\n    @content;\n  }\n}\n\n@mixin ellipsis {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n","// Colors here are deprecated! Please use new variables in the variables.scss\n// If you need to use some of these variables please cont