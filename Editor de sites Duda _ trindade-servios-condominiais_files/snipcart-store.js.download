  &-footer {\n        @include typography-12r();\n    }\n    &-display-00 {\n        @include typography-60b();\n    }\n    &-display-0 {\n        @include typography-40b();\n    }\n    &-display-1 {\n        @include typography-32b();\n    }\n    &-display-2 {\n        @include typography-24m();\n    }\n    &-display-3 {\n        @include typography-20m();\n    }\n    &-display-4 {\n        @include typography-10b-cap();\n    }\n    &-description {\n        @include typography-20r();\n    }\n    &-he-1 {\n        @include typography-16b();\n    }\n    &-he-2 {\n        @include typography-14m();\n    }\n    &-he-2-bold {\n        @include typography-14b();\n    }\n    &-he-3 {\n        @include typography-13m();\n    }\n    &-he-4 {\n        @include typography-12m();\n    }\n    &-plain-text {\n        @include typography-16r();\n    }\n    &-body-text {\n        @include typography-13r();\n    }\n    &-body-bold-text {\n        @include typography-13m();\n    }\n    &-tiny-text {\n        @include typography-12r();\n    }\n    &-tiny-bold-text {\n        @include typography-12m();\n    }\n    &-small-buttons {\n        @include typography-12m();\n    }\n    &-large-buttons {\n        @include typography-14m();\n    }\n    &-mini {\n        @include typography-10r();\n    }\n}\n","@import 'deprecated-colors';\n@import 'deprecated-variables';\n@import 'primitive-colors';\n\n//Custom branding colors\n$primary-color: var(--button-color);\n$secondary-color: var(--text-on-light);\n$text-on-primary-color: var(--text-on-button);\n$top-bar-color: var(--header-color);\n\n// Platform colors\n$bg-white-color: $white;\n$bg-grey-color: $grey-100;\n$bg-med-grey-color: $grey-200;\n$bg-off-white-color: $off-white-50;\n$bg-med-off-white-color: $off-white-100;\n$bg-dark-grey-color: $grey-500;\n\n// Border colors\n$border-color: $grey-300;\n$border-light-color: $grey-200;\n$border-dark-color: $grey-600;\n$active-border-color: $grey-400;\n$divider-color: $grey-200;\n$border-white-color: $white;\n$border-off-white-color: $off-white-300;\n\n// deprecated colors\n$selectable-item-color: $grey-600;\n$error-color: $red-500;\n$error-bg-color: $red-50;\n$success-color: $green-400;\n$success-bg-color: $green-50;\n\n// Typography colors\n$primary-text-color: $grey-800;\n$secondary-text-color: $grey-600;\n$hint-color: $grey-500;\n$text-white-color: $white;\n\n// Icon color\n$icon-color: $grey-600;\n$icon-dark-color: $grey-800;\n$icon-light-color: $white;\n\n// Indication\n$link-color: $blue-600;\n$disabled-item-color: $grey-300;\n$positive-light-color: $green-50;\n$positive-subtle-color: $green-100;\n$positive-color: $green-500;\n$positive-med-color: $green-700;\n$positive-dark-color: $green-800;\n$negative-light-color: $red-50;\n$negative-subtle-color: $red-100;\n$negative-color: $red-600;\n$negative-med-color: $red-700;\n$negative-dark-color: $red-800;\n$info-bg-color: $blue-50;\n$info-color: $blue-400;\n$info-med-color: $blue-700;\n$info-dark-color: $blue-800;\n$ai-bg-color: $purple-50;\n$ai-color: $purple-500;\n$ai-med-color: $purple-100;\n$ai-dark-color: $purple-700;\n$warning-color: $yellow-500;\n$warning-med-color: $yellow-300;\n$warning-bg-color: $yellow-100;\n$flex-color: $blue-500;\n$flex-dark-color: $blue-600;\n$pending-bg-color: $orange-100;\n$pending-color: $orange-600;\n\n\n// Component colors - should not be used anywhere else apart from table\n$header-color: $grey-100;\n$table-cell-hover-color: $grey-100;\n$exit-button-color: $grey-600;\n\n//Tag labels\n$tag-lilac-color: $purple-300;\n$tag-red-color: $pink-400;\n$tag-orange-color: $orange-400;\n$tag-light-green-color: $green-400;\n$tag-dark-blue-color: $blue-200;\n$tag-dark-green-color: $green-600;\n$tag-grey-color: $grey-600;\n$tag-purple-color: $purple-700;\n$tag-light-grey-color: $grey-200;\n\n// Status colors\n$status-dark-grey-bg-color: $grey-300;\n$status-dark-grey-color: $grey-700;\n$status-dark-grey-border-color: $grey-400;\n$status-neutral-bg-color: $grey-100;\n$status-neutral-color: $grey-600;\n$status-neutral-border-color: $grey-200;\n$status-positive-bg-color: $green-50;\n$status-positive-color: $green-700;\n$status-positive-border-color: $green-100;\n$status-error-bg-color: $red-50;\n$status-error-color: $red-700;\n$status-error-border-color: $red-100;\n$status-purple-bg-color: $purple-50;\n$status-purple-color: $purple-700;\n$status-purple-border-color: $purple-100;\n$status-pending-bg-color: $orange-50;\n$status-pending-color: $orange-700;\n$status-pending-border-color: $orange-100;\n$status-turquoise-bg-color: $blue-50;\n$status-turquoise-color: $blue-800;\n$status-turquoise-border-color: $blue-100;\n\n// GENERIC SHADOWS\n$shadow-base-color: #18274b;\n$shadow-1: 0px 2.25px 2.25px -1.125px rgba($shadow-base-color, 0.04),\n    0px 1.125px 2.25px -1.125px rgba($shadow-base-color, 0.08);\n\n$shadow-2: 0px 4.5px 9px -3.375px rgba($shadow-base-color, 0.04),\n    0px 3.375px 4.5px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-3: 0px 5.625px 18px -2.25px rgba($shadow-base-color, 0.06),\n    0px 3.375px 7.875px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-4: 0px 7.875px 36px -2.25px rgba($shadow-base-color, 0.08),\n    0px 4.5px 12.375px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-5: 0px 10.125px 49.5px -2.25px rgba($shadow-base-color, 0.12),\n    0px 4.5px 15.75px -3.375px rgba($shadow-base-color, 0.1);\n\n$top-bar-items-depths: base, buttons, banners;\n\n// SPACING\n$base-spacing: 8px;\n$spacing-0: 0;\n$spacing-half: 4px;\n$spacing-1: $spacing-half * 2;\n$spacing-2: $spacing-1 * 2;\n$spacing-3: $spacing-1 * 3;\n$spacing-4: $spacing-1 * 4;\n$spacing-5: $spacing-1 * 5;\n$spacing-6: $spacing-1 * 6;\n$spacing-7: $spacing-1 * 7;\n$spacing-8: $spacing-1 * 8;\n\n// BORDERS\n$default-border-radius: 4px;\n$floating-elements-radius: 6px;\n$default-border: 1px solid $border-color;\n$active-border: 1px solid $active-border-color;\n\n// FONTS\n$duda-title-font: var(--d-title-font, 'Roboto');\n$duda-font: var(--d-font, 'Source Sans Pro', sans-serif);\n\n// typography font sizes\n$font-size-h1: var(--font-size-h1, 48px);\n$font-size-h2: var(--font-size-h2, 40px);\n$font-size-h3: var(--font-size-h3, 32px);\n$font-size-h4: var(--font-size-h4, 24px);\n$font-size-h5: var(--font-size-h5, 20px);\n$font-size-h6: var(--font-size-h6, 16px);\n$font-size-pageDescription: var(--font-size-pageDescription, 16px);\n$font-size-bodySubtitle: var(--font-size-bodySubtitle, 14px);\n$font-size-body: var(--font-size-body, 14px);\n$font-size-italic: var(--font-size-italic, 14px);\n$font-size-tiny: var(--font-size-tiny, 13px);\n$font-size-footer: var(--font-size-footer, 12px);\n\n// new font-sizes:\n$font-size-display-1: var(--font-size-display-1, 32px);\n$font-size-display-2: var(--font-size-display-1, 24px);\n$font-size-display-3: var(--font-size-display-1, 20px);\n$font-size-display-4: var(--font-size-display-1, 10px);\n$font-size-he-1: var(--font-size-he-1, 16px);\n$font-size-he-2: var(--font-size-he-2, 14px);\n$font-size-he-3: var(--font-size-he-3, 13px);\n$font-size-he-4: var(--font-size-he-4, 12px);\n$font-size-plain-text: var(--font-size-plain-text, 16px);\n$font-size-body-text: var(--font-size-body-text, 13px);\n$font-size-tiny-text: var(--font-size-tiny-text, 12px);\n$font-size-mini: var(--font-size-mini, 10px);\n\n$widget-width: 344px;\n$widget-width-mid-size: 449px;\n$widget-width-large-size: 616px;\n$widget-double-width: 2 * $widget-width;\n$widget-width-xl-size: 963px;\n$widget-height: 536px;\n\n$default-widget-padding: 31px;\n\n$container-width-regular-size: $widget-width - $default-widget-padding;\n$container-width-large-size: $widget-width-large-size - $default-widget-padding;\n$container-width-double-size: $widget-double-width - $default-widget-padding;\n\n$left-panel-inner-width: 302px;\n\n$settings-panel-large: 617px;\n\n$editor-left-panel-width: 98px;\n\n$top-bar-height: 50px;\n$top-bar-message-height: 50px;\n\n$input-height: 35px;\n\n// for tablet portrait\n$tablet-editing-breakpoint: 1000px;\n$zo