    (function initExperimentalFeatures() {
    const experimentalFlags = {};
    const overrides = {};
    $.dmExperimental = {
        /*
         * reload the overrides from the local cache
         */
        reloadFlagsFromLocalCache: function() {
            const overridesFromCache = $.storage.get('dmExperimentalOverrides');
            if (overridesFromCache) {
                const overridesObj = JSON.parse(overridesFromCache) || {};
                $.each(overridesObj, function(key, overrideValue) {
                    // override only if it's experimental
                    if (experimentalFlags[key]) {
                        overrides[key] = overrideValue;
                        commonProps[key] = overrideValue;
                    }
                });
            }
        },
        /*
         * initialize a flag to be experimental
         */
        setExperimental: function(key, desc, prop) {
            prop = prop || {};

            // define initial value
            let initialValue = prop.initial;
            if (initialValue === undefined) {
                initialValue = commonProps[key] || false;
            }

            experimentalFlags[key] = {
                desc: desc || key,
                additionalDesc: prop.additionalDesc,
                initial: initialValue,
                isNew: prop.isNew,
                server: prop.server,
                contextMenu: prop.contextMenu,
                hide_wl: prop.hide_wl,
                refresh: prop.refresh === undefined ? true : !!prop.refresh,
                html: prop.html
            };
            commonProps[key] = initialValue;
        },
        /*
         * initialize multiple flags as experimental
         */
        setExperimentals: function(expObj) {
            expObj = expObj || {};
            let expArray = [];
            const isDevUser = commonProps['editor.experimental.showDevFeatures'];
            const isExperimentalRoleUser = commonProps['editor.experimental.userHasExperimentalRole'];

            // add role-based flags
            if (isDevUser || isExperimentalRoleUser) {
                expArray = expArray.concat(expObj.PROD_LIMITED || []);
            }

            // add public flags
            expArray = expArray.concat(expObj.PUBLIC || []);

            // filter out non-wl props
            if (!isDevUser) {
                expArray.forEach(function(prop) {
                    if (!prop.show_wl) {
                        prop.hide_wl = true;
                    }
                });
                if (window.uiPerms && !uiPerms.D_AWARE) {
                    expArray = expArray.filter(function(prop) {
                        return prop.show_wl;
                    });
                }
            }

            // add dev only flags
            if (isDevUser) {
                expArray = expArray.concat(expObj.DEV_ONLY || []);
            }

            expArray.forEach(function(prop) {
                if (!prop) {
                    return;
                }

                if (prop.hide_wl && window.uiPerms && !uiPerms.D_AWARE) {
                    return;
                }

                if (prop.hidden) {
                    return;
                }

                $.dmExperimental.setExperimental(prop.key, prop.desc || prop.key, prop);
            });
        },
        /*
         * get a flag
         */
        get: function(key) {
            return commonProps[key];
        },
        /*
         * store new overrides in local cache
         */
        store: function(keyValues) {
            const changedData = {};
            $.each(keyValues || {}, function(key, newValue) {
                changedData[key] = $.extend({}, experimentalFlags[key], {
                    newValue: newValue
                });
                // trigger events
                $.dmx.events.trigger('commonPropChanged:' + key.replace(/\./g, '_'), changedData[key]);
                // track in mixpanel
                wrTrackEvent('d1-experimental-change', {
                    feature: key + '_' + changedData[key].newValue
                });
                wrTrackEvent(changedData[key].newValue ? 'd1-experimental-on' : 'd1-experimental-off', {
                    feature: key
                });

                overrides[key] = newValue;
                commonProps[key] = newValue;
                if (newValue === experimentalFlags[key].initial) delete overrides[key];
            });
            // generate server properties
            const serverOverrides = {};
            $.each(overrides, function(key, value) {
                if ((experimentalFlags[key] || {}).server) {
                    serverOverrides[key] = value;
                }
            });

            $.storage.set('dmExperimentalOverrides', JSON.stringify(overrides));
            $.dmCookies.set('experimentalOverrides', JSON.stringify(serverOverrides), 24 * 7);
            $.dmx.events.trigger('commonPropsChanged', changedData);
        },
        /*
         * get all the experimental flags
         */
        getAllFlags: function() {
            return experimentalFlags;
        },
        /*
         * get all the overrides flags
         */
        getAllOverrides: function() {
            return overrides;
        },
        /*
         * check if overrides are present
         */
        hasOverrides: function() {
            return !!Object.keys($.dmExperimental.getAllOverrides()).length;
        },
        /*
         * check if any flags are visible
         */
        hasFlags: function() {
            return !!Object.keys($.dmExperimental.getAllFlags()).length;
        }
    };
})();

function setPropertiesFromString(propsOverride) {
    if (!propsOverride) {
        return;
    }
    try {
        (propsOverride || '').split(',').forEach(function(kv) {
            const pair = kv.split(':') || [];
            if (pair.length === 2) {
                const val = pair[1] || '';
                const ind = ['false', 'true'].indexOf(val);
                commonProps[pair[0]] = [val, false, true][ind + 1];
            }
        });
    } catch (e) {}
}


    
