lue-100;\n\n// GENERIC SHADOWS\n$shadow-base-color: #18274b;\n$shadow-1: 0px 2.25px 2.25px -1.125px rgba($shadow-base-color, 0.04),\n    0px 1.125px 2.25px -1.125px rgba($shadow-base-color, 0.08);\n\n$shadow-2: 0px 4.5px 9px -3.375px rgba($shadow-base-color, 0.04),\n    0px 3.375px 4.5px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-3: 0px 5.625px 18px -2.25px rgba($shadow-base-color, 0.06),\n    0px 3.375px 7.875px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-4: 0px 7.875px 36px -2.25px rgba($shadow-base-color, 0.08),\n    0px 4.5px 12.375px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-5: 0px 10.125px 49.5px -2.25px rgba($shadow-base-color, 0.12),\n    0px 4.5px 15.75px -3.375px rgba($shadow-base-color, 0.1);\n\n$top-bar-items-depths: base, buttons, banners;\n\n// SPACING\n$base-spacing: 8px;\n$spacing-0: 0;\n$spacing-half: 4px;\n$spacing-1: $spacing-half * 2;\n$spacing-1-half: $spacing-1 + $spacing-half;\n$spacing-2: $spacing-1 * 2;\n$spacing-3: $spacing-1 * 3;\n$spacing-4: $spacing-1 * 4;\n$spacing-5: $spacing-1 * 5;\n$spacing-6: $spacing-1 * 6;\n$spacing-7: $spacing-1 * 7;\n$spacing-8: $spacing-1 * 8;\n$spacing-2-half: 18px;\n\n// BORDERS\n/**\n* @deprecated please use $border-radius-s instead\n */\n$default-border-radius: 4px;\n$border-radius-xs: 2px;\n$border-radius-s: 4px;\n$border-radius-sm: 6px;\n$border-radius-m: 8px;\n$border-radius-l: 12px;\n$border-radius-full: 1000px;\n/**\n* @deprecated please use $border-radius-s or $border-radius-m instead, we don't have 6px radius anymore\n */\n$floating-elements-radius: 6px;\n$default-border: 1px solid $border-color;\n$active-border: 1px solid $active-border-color;\n\n// FONTS\n$duda-title-font: var(--d-title-font, 'Roboto');\n$duda-font: var(--d-font, 'Source Sans Pro', sans-serif);\n\n// typography font sizes\n$font-size-h1: var(--font-size-h1, 48px);\n$font-size-h2: var(--font-size-h2, 40px);\n$font-size-h3: var(--font-size-h3, 32px);\n$font-size-h4: var(--font-size-h4, 24px);\n$font-size-h5: var(--font-size-h5, 20px);\n$font-size-h6: var(--font-size-h6, 16px);\n$font-size-pageDescription: var(--font-size-pageDescription, 16px);\n$font-size-bodySubtitle: var(--font-size-bodySubtitle, 14px);\n$font-size-body: var(--font-size-body, 14px);\n$font-size-italic: var(--font-size-italic, 14px);\n$font-size-tiny: var(--font-size-tiny, 13px);\n$font-size-footer: var(--font-size-footer, 12px);\n\n// new font-sizes:\n$font-size-display-1: var(--font-size-display-1, 32px);\n$font-size-display-2: var(--font-size-display-1, 24px);\n$font-size-display-3: var(--font-size-display-1, 20px);\n$font-size-display-4: var(--font-size-display-1, 10px);\n$font-size-he-1: var(--font-size-he-1, 16px);\n$font-size-he-2: var(--font-size-he-2, 14px);\n$font-size-he-3: var(--font-size-he-3, 13px);\n$font-size-he-4: var(--font-size-he-4, 12px);\n$font-size-plain-text: var(--font-size-plain-text, 16px);\n$font-size-body-text: var(--font-size-body-text, 13px);\n$font-size-tiny-text: var(--font-size-tiny-text, 12px);\n$font-size-mini: var(--font-size-mini, 10px);\n\n$widget-width: 344px;\n$widget-width-mid-size: 449px;\n$widget-width-large-size: 616px;\n$widget-double-width: 2 * $widget-width;\n$widget-width-xl-size: 963px;\n$widget-height: 536px;\n\n$default-widget-padding: 31px;\n\n$container-width-regular-size: $widget-width - $default-widget-padding;\n$container-width-large-size: $widget-width-large-size - $default-widget-padding;\n$container-width-double-size: $widget-double-width - $default-widget-padding;\n\n$left-panel-inner-width: 302px;\n\n$settings-panel-large: 617px;\n\n$editor-left-panel-width: var(--editor-left-panel-width, 98px);\n\n$top-bar-height: 50px;\n$top-bar-message-height: 50px;\n\n$input-height: 35px;\n\n// for tablet portrait\n$tablet-editing-breakpoint: 1000px;\n$zoom-coefficient: 1.5;\n$landscape-media-query: 'screen and (min-width: $tablet-editing-breakpoint), screen and (orientation: landscape)';\n$left-panel-width: 800px;\n$left-panel-list-width: 270px;\n$le