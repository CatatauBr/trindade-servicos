eclectic-8, $eclectic-8);\n$baby-blue-color: var(--eclectic-9, $eclectic-9);\n$lemon-yellow-color: var(--eclectic-10, $eclectic-10);\n$olive-green-color: var(--eclectic-11, $eclectic-11);\n$bright-green-color: var(--eclectic-12, $eclectic-12);\n$light-orange-color: var(--eclectic-13, $eclectic-13);\n$baby-pink-color: var(--eclectic-14, $eclectic-14);\n$lilach-color: var(--eclectic-15, $eclectic-15);\n$light-purple-color: var(--eclectic-16, $eclectic-16);\n$light-green-color: var(--eclectic-17, $eclectic-17);\n$light-olive-color: var(--eclectic-18, $eclectic-18);\n$light-yellow-color: var(--eclectic-19, $eclectic-19);\n\n// Component colors - should not be used anywhere else apart from table\n$header-color: $grey-100;\n$table-cell-hover-color: $grey-100;\n$exit-button-color: $grey-600;\n\n//Tag labels\n$tag-lilac-color: $purple-300;\n$tag-red-color: $pink-400;\n$tag-orange-color: $orange-400;\n$tag-light-green-color: $green-400;\n$tag-dark-blue-color: $blue-200;\n$tag-dark-green-color: $green-600;\n$tag-grey-color: $grey-600;\n$tag-purple-color: $purple-700;\n$tag-light-grey-color: $grey-100;\n\n// Status colors\n$status-yellow-bg-color: $yellow-100;\n$status-yellow-color: $orange-700;\n$status-yellow-border-color: $yellow-300;\n$status-dark-grey-bg-color: $grey-300;\n$status-dark-grey-color: $grey-700;\n$status-dark-grey-border-color: $grey-400;\n$status-neutral-bg-color: $grey-100;\n$status-neutral-color: $grey-600;\n$status-neutral-border-color: $grey-200;\n$status-positive-bg-color: $green-50;\n$status-positive-color: $green-700;\n$status-positive-border-color: $green-100;\n$status-error-bg-color: $red-50;\n$status-error-color: $red-700;\n$status-error-border-color: $red-100;\n$status-purple-bg-color: $purple-50;\n$status-purple-color: $purple-700;\n$status-purple-border-color: $purple-100;\n$status-pending-bg-color: $orange-50;\n$status-pending-color: $orange-700;\n$status-pending-border-color: $orange-100;\n$status-turquoise-bg-color: $blue-50;\n$status-turquoise-color: $blue-800;\n$status-turquoise-border-color: $blue-100;\n\n// GENERIC SHADOWS\n$shadow-base-color: #18274b;\n$shadow-1: 0px 2.25px 2.25px -1.125px rgba($shadow-base-color, 0.04),\n    0px 1.125px 2.25px -1.125px rgba($shadow-base-color, 0.08);\n\n$shadow-2: 0px 4.5px 9px -3.375px rgba($shadow-base-color, 0.04),\n    0px 3.375px 4.5px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-3: 0px 5.625px 18px -2.25px rgba($shadow-base-color, 0.06),\n    0px 3.375px 7.875px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-4: 0px 7.875px 36px -2.25px rgba($shadow-base-color, 0.08),\n    0px 4.5px 12.375px -3.375px rgba($shadow-base-color, 0.08);\n\n$shadow-5: 0px 10.125px 49.5px -2.25px rgba($shadow-base-color, 0.12),\n    0px 4.5px 15.75px -3.375px rgba($shadow-base-color, 0.1);\n\n$top-bar-items-depths: base, buttons, banners;\n\n// SPACING\n$base-spacing: 8px;\n$spacing-0: 0;\n$spacing-half: 4px;\n$spacing-1: $spacing-half * 2;\n$spacing-1-half: $spacing-1 + $spacing-half;\n$spacing-2: $spacing-1 * 2;\n$spacing-3: $spacing-1 * 3;\n$spacing-4: $spacing-1 * 4;\n$spacing-5: $spacing-1 * 5;\n$spacing-6: $spacing-1 * 6;\n$spacing-7: $spacing-1 * 7;\n$spacing-8: $spacing-1 * 8;\n$spacing-2-half: 18px;\n\n// BORDERS\n/**\n* @deprecated please use $border-radius-s instead\n */\n$default-border-radius: 4px;\n$border-radius-xs: 2px;\n$border-radius-s: 4px;\n$border-radius-sm: 6px;\n$border-radius-m: 8px;\n$border-radius-l: 12px;\n$border-radius-full: 1000px;\n/**\n* @deprecated please use $border-radius-s or $border-radius-m instead, we don't have 6px radius anymore\n */\n$floating-elements-radius: 6px;\n$default-border: 1px solid $border-color;\n$active-border: 1px solid $active-border-color;\n\n// FONTS\n$duda-title-font: var(--d-title-font, 'Roboto');\n$duda-font: var(--d-font, 'Source Sans Pro', sans-serif);\n\n// typography font sizes\n$font-size-h1: var(--font-size-h1, 48px);\n$font-size-h2: var(--font-size-h2, 40px);\n$font-size-h3: var(--font-size-h3, 32px);\n$font-size-h4: var(--font-size-h4, 24px);\n$font-size-h5: var(--font-size-h5, 20px);\n$font-size-h6: var(--font-size-h6, 16px);\n$font-size-pageDescription: var(--font-size-pageDescription, 16px);\n$font-size-bodySubtitle: var(--font-size-bodySubtitle, 14px);\n$font-size-body: var(--font-size-body, 14px);\n$font-size-italic: var(--font-size-italic, 14px);\n$font-size-tiny: var(--font-size-tiny, 13px);\n$font-size-footer: var(--font-size-footer, 12px);\n\n// new font-sizes:\n$font-size-display-1: var(--font-size-display-1, 32px);\n$font-size-display-2: var(--font-size-display-1, 24px);\n$font-size-display-3: var(--font-size-display-1, 20px);\n$font-size-display-4: var(--font-size-display-1, 10px);\n$font-size-he-1: var(--font-size-he-1, 16px);\n$font-size-he-2: var(--font-size-he-2, 14px);\n$font-size-he-3: var(--font-size-he-3, 13px);\n$font-size-he-4: var(--font-size-he-4, 12px);\n$font-size-plain-text: var(--font-size-plain-text, 16px);\n$font-size-body-text: var(--font-size-body-text, 13px);\n$font-size-tiny-text: var(--font-size-tiny-text, 12px);\n$font-size-mini: var(--font-size-mini, 10px);\n\n$widget-width: 344px;\n$widget-width-mid-size: 449px;\n$widget-width-large-size: 616px;\n$widget-double-width: 2 * $widget-width;\n$widget-width-xl-size: 963px;\n$widget-height: 536px;\n\n$default-widget-padding: 31px;\n\n$container-width-regular-size: $widget-width - $default-widget-padding;\n$container-width-large-size: $widget-width-large-size - $default-widget-padding;\n$container-width-double-size: $widget-double-width - $default-widget-padding;\n\n$left-panel-inner-width: 302px;\n\n$settings-panel-large: 617px;\n\n$editor-left-panel-width: var(--editor-left-panel-width, 98px);\n\n$top-bar-height: 50px;\n$top-bar-message-height: 50px;\n\n$input-height: 35px;\n\n// for tablet portrait\n$tablet-editing-breakpoint: 1000px;\n$zoom-coefficient: 1.5;\n$landscape-media-query: 'screen and (min-width: $tablet-editing-breakpoint), screen and (orientation: landscape)';\n$left-panel-width: 800px;\n$left-panel-list-width: 270px;\n$left-panel-right-width: $left-panel-width - $left-panel-list-width - 50px;\n\n// duda-go\n$default-padding: 24px;\n$default-widget-padding-top: 16px;\n$default-mobile-shadow: 0 -2px 12px 0 rgba(47, 55, 58, 0.5);\n$default-mobile-shadow-top: 0 2px 12px 0 rgba(47, 55, 58, 0.5);\n$input-border-bottom: 1px solid $dark-gray;\n$mobile-popups-box-shadow: 1px 1px 21px rgba(0, 0, 0, 0.3);\n$default-panels-border-radius: 9px;\n\n//right-design-panel\n$right-design-panel-rows-margin: 20px;\n\n// overview\n$overview-pages-panel-width: 266px;\n\n// MIXINS\n@mixin rotate($deg: 90deg) {\n    -webkit-transform: rotate($deg);\n    -moz-transform: rotate($deg);\n    -ms-transform: rotate($deg);\n    -o-transform: rotate($deg);\n    transform: rotate($deg);\n}\n\n@mixin transition($length: 1s, $delay: 0s, $easing: ease, $properties: all) {\n    transition: $length $easing $delay $properties;\n}\n","@import '~@duda-co/styles/variables';\n@import '~@duda-co/styles/basic-typography';\n\n.small {\n    @include typography-12m();\n    height: 24px;\n    min-width: 72px;\n\n    .start-icon {\n        padding-inline-end: 2px;\n    }\n    .end-icon {\n        padding-inline-start: 2px;\n    }\n    padding-inline-start: 14px;\n    padding-inline-end: 14px;\n\n    &.has-start-icon {\n        padding-inline-start: 12px;\n    }\n    &.has-end-icon {\n        padding-inline-end: 12px;\n    }\n}\n\n.regular {\n    @include typography-14m();\n    height: 32px;\n    .start-icon {\n        padding-inline-end: 4px;\n    }\n    .end-icon {\n        padding-inline-start: 4px;\n    }\n}\n","@import './variables';\n$cal-font: var(--d-cal-font, 'Cal Sans', 'Label Sans', sans-serif);\n\n@mixin typography-60b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-display-00, 60px);\n    font-weight: 700;\n    line-height: 60px;\n}\n\n@mixin typography-40b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-display-0, 40px);\n    font-weight: 700;\n    line-height: 49px;\n}\n\n@mixin typography-32b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-display-1, 32px);\n    font-weight: 700;\n    line-height: 40px;\n}\n\n@mixin typography-24m() {\n    font-family: $duda-font;\n    font-size: var(--font-size-display-2, 24px);\n    font-weight: 550;\n    line-height: 30px;\n}\n\n@mixin typography-20m() {\n    font-family: $duda-font;\n    font-size: var(--font-size-display-3, 20px);\n    font-weight: 550;\n    line-height: 26px;\n}\n\n@mixin typography-20r() {\n    font-family: $duda-font;\n    font-size: var(--font-size-description, 20px);\n    font-weight: 350;\n    line-height: 26px;\n}\n\n@mixin typography-18m() {\n    font-family: $duda-font;\n    font-size: 18px;\n    font-weight: 550;\n    line-height: 24px;\n}\n\n@mixin typography-16b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-he-1, 16px);\n    font-weight: 700;\n    line-height: 22px;\n}\n\n@mixin typography-16r() {\n    font-family: $duda-font;\n    font-size: var(--font-size-plain-text, 16px);\n    font-weight: 350;\n    line-height: 22px;\n}\n\n@mixin typography-14m() {\n    font-family: $duda-font;\n    font-size: var(--font-size-he-2, 14px);\n    font-weight: 550;\n    line-height: 20px;\n}\n\n@mixin typography-14b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-he-2, 14px);\n    font-weight: 700;\n    line-height: 20px;\n}\n\n@mixin typography-14r() {\n    font-family: $duda-font;\n    font-size: var(--font-size-he-2, 14px);\n    font-weight: 350;\n    line-height: 20px;\n}\n\n@mixin typography-13b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-body-text, 13px);\n    font-weight: 700;\n    line-height: 18px;\n}\n\n@mixin typography-13m() {\n    font-family: $duda-font;\n    font-size: var(--font-size-he-3, 13px);\n    font-weight: 550;\n    line-height: 18px;\n}\n\n@mixin typography-13r() {\n    font-family: $duda-font;\n    font-size: var(--font-size-body-text, 13px);\n    font-weight: 350;\n    line-height: 18px;\n}\n\n@mixin typography-12b() {\n    font-family: $duda-font;\n    font-size: var(--font-size-tiny-text, 12px);\n    font-weight: 700;\n    line-height: 18px;\n}\n\n@mixin typography-12m() {\n    font-family: $duda-font;\n    font-size: var(--font-size-he-4, 12px);\n    font-weight: 550;\n    line-height: 18px;\n}\n\n@mixin typography-12r() {\n    font-family: $duda-font;\n    font-size: var(--font-size-tiny-text, 12px);\n    font-weight: 350;\n    line-height: 18px;\n}\n\n@mixin typography-11m() {\n    font-family: $duda-font;\n    font-size: var(--font-size-mini, 11px);\n    font-weight: 550;\n    line-height: 16px;\n}\n\n@mixin typography-10r() {\n    font-family: $duda-font;\n    font-size: var(--font-size-mini, 10px);\n    font-weight: 350;\n    line-height: 16px;\n}\n\n@mixin typography-10b-cap() {\n    font-family: $duda-font;\n    font-size: var(--font-size-display-4, 10px);\n    font-weight: 700;\n    line-height: 14px;\n    text-transform: uppercase;\n}\n\n@mixin typography-cal60() {\n    font-family: $cal-font;\n    font-size: 60px;\n    line-height: 68px;\n}\n\n@mixin typography-cal40() {\n    font-family: $cal-font;\n    font-size: 40px;\n    line-height: 48px;\n}\n\n@mixin typography-cal32() {\n    font-family: $cal-font;\n    font-size: 32px;\n    line-height: 40px;\n}\n\n@mixin typography-cal24() {\n    font-family: $cal-font;\n    font-size: 24px;\n    line-height: 28px;\n}\n\n@mixin typography-cal20() {\n    font-family: $cal-font;\n    font-size: 20px;\n    line-height: 24px;\n}\n\n@mixin typography-cal16() {\n    font-family: $cal-font;\n    font-size: 16px;\n    line-height: 20px;\n}\n\n@mixin typography-cal12-cap() {\n    font-family: $cal-font;\n    font-size: 12px;\n    line-height: 16px;\n    letter-spacing: 0.05em;\n    text-transform: uppercase;\n}\n","@import '~@duda-co/styles/variables';\n\n.primary {\n    color: $text-on-primary-color;\n    &::before {\n        background-color: $p